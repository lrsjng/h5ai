# editorconfig.org

root = true


[*]
charset = utf-8
indent_style = space
indent_size = 4
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true


[{package.json,.travis.yml,.eslintrc,.babelrc}]
indent_size = 2


[{*.md,*.jade}]
trim_trailing_whitespace = false


[*.svg]
insert_final_newline = false
