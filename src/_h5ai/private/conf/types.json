    "x-disc":           { "glob": ["*.cue", "*.iso"] },
    "x-doc":            { "glob": ["*.doc", "*.docx", "*.odm", "*.odt", "*.ott"] },
    "x-draw":           { "glob": ["*.drw"] },
    "x-eps":            { "glob": ["*.eps"] },
    "x-pdf":            { "glob": ["*.pdf", "*.gs"], "mime": ["pdf", "ghostscript"] },
    "x-pres":           { "glob": ["*.odp", "*.otp", "*.pps", "*.ppt", "*.pptx"] },
    "x-ps":             { "glob": ["*.ps"] },
    "x-psd":            { "glob": ["*.psd"] }
}
